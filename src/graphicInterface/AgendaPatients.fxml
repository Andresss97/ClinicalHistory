<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>

<BorderPane fx:id="container" prefHeight="509.0" prefWidth="855.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="graphicInterface.ControllerAgendaPatients">
   <left>
      <Region prefHeight="473.0" prefWidth="38.0" BorderPane.alignment="CENTER" />
   </left>
   <right>
      <Region prefHeight="473.0" prefWidth="34.0" BorderPane.alignment="CENTER" />
   </right>
   <top>
      <HBox BorderPane.alignment="CENTER">
         <children>
            <JFXButton fx:id="pWeek" onAction="#onPreviousWeekClick" prefHeight="25.0" prefWidth="96.0" style="-fx-background-color: #249ce8;" text="Previous Week" textFill="WHITE">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </JFXButton>
            <JFXButton fx:id="nWeek" onAction="#onNextWeekClick" style="-fx-background-color: #249ce8;" text="Next Week" textFill="WHITE" />
            <Region HBox.hgrow="ALWAYS" />
            <JFXButton fx:id="day" onAction="#onClickDayView" style="-fx-background-color: #249ce8;" text="Day View" textFill="WHITE" />
            <JFXButton fx:id="week" onAction="#onClickWeekView" style="-fx-background-color: #249ce8;" text="Week Viiew" textFill="WHITE">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" />
               </HBox.margin>
            </JFXButton>
            <JFXButton fx:id="month" onAction="#onClickMonthView" style="-fx-background-color: #249ce8;" text="Month View" textFill="WHITE" />
         </children>
         <padding>
            <Insets bottom="10.0" left="38.0" right="34.0" top="20.0" />
         </padding>
      </HBox>
   </top>
   <bottom>
      <Region prefHeight="26.0" prefWidth="855.0" BorderPane.alignment="CENTER" />
   </bottom>
</BorderPane>
